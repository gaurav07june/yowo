<resources>

    <style name="AppTheme.WhiteActionBar" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorWhite</item>
        <item name="android:statusBarColor">@color/colorWhite</item>
        <item name="colorPrimaryDark">@color/colorPrimaryB</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>
    <style name="AppTheme.White.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorWhite</item>
        <item name="android:statusBarColor">@color/colorWhite</item>
        <item name="colorPrimaryDark">@color/colorPrimaryB</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>
</resources>
